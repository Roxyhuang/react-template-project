{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    ["es2015"],
    "react",
    "stage-0"
  ],
  "plugins":[
    [
      "import", [
        { "libraryName": "antd", "style": true },
        { "libraryName": "antd-mobile", "style": true }
      ],
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  ]
}